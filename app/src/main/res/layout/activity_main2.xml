<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <ImageView
        android:id="@+id/dubai"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginRight="-320dp"
        android:src="@drawable/Dubaihospital"/>

    <ImageView
        android:id="@+id/latifa"
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:src="@drawable/Latifahospital"
        tools:layout_editor_absoluteX="300dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_marginRight="-50dp"
        android:layout_marginLeft="128dp"/>

    <ImageView
        android:id="@+id/Rashid"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:src="@drawable/Rashidhospital"
        tools:layout_editor_absoluteX="-40dp"
        tools:layout_editor_absoluteY="765dp"
        android:layout_marginRight="-50dp"
        android:layout_marginLeft="250dp"/>

    <TextView
        android:id="@+id/Dubaihospital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="80dp"
        android:text="Dubai Hospital"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Latifahospital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="470dp"
        android:text="Latifa Hospital"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Rashidhospital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="850dp"
        android:text="RashidHospital"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>